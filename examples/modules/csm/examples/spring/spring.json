// -*- mode: javascript -*-
{
  "Name": "Solid Mechanics ",
  "ShortName":"Solid",
  "Metadata":
  {
      "version": "1",
      "author": ["vincentchabannes","prudhomm","lberti","jobinhio"],
      "date": "30/11/2022",
      "references":[""]
  },
  "Models":
  {
    "solid":
    {
      "setup":
      {
        "equations":"Elasticity", // "Elasticity", "Hyper-Elasticity"
        "body-forces":"{0,0,-gravity*7850}:gravity"
      }
    }  
  },
  "Meshes":
  {
      "solid":
      {
        "Import":
        {
          "filename":"girder:{file:5b03dfcbb0e957402704806d}"
        }
      }
  },
  // tag::materials[]
  "Materials":
  {
    "lumenVolume":
    {
      //#acier : http://www.simulationmateriaux.com/ComportementMecanique/comportement_mecanique_Liste_modules_de_Young.php
      "name":"steel",
      "E":"210e6", // [kg.m^{-1}.s^{-2}] (Young's modulus)
      "nu":"0.33", // dimensionless
      "rho":"7850" // [kg/m^3]
    }
  },
  // end::materials[]
  // tag::params[]
  "Parameters":
  {
    "dispImposed":
    {
      "value":"8e-3" // [m]
    },
    "gravity":
    {
      "value":"9.80665" // [m.s^{-2}]
    }
  },
  // end::params[]
  // tag::bc[]
  "BoundaryConditions":
  {
    "solid":
    {
      "displacement":
      {
        "markerBottom":{ "expr":"{0,0,-dispImposed}:dispImposed" }, // imposed displacement -> pulled down end of the spring
        "markerTop":{ "expr":"{0,0,0}" } // imposed displacement (zero) -> fixed end of the spring
      }
    }
  },
  // end::bc[]
  // tag::export[]
  "PostProcess":
  {
    "solid":
    {
      "Exports":
      {
        "fields":["displacement","pressure","pid","von-mises-criterion"]
      },
      "Measures":
      {
        "Statistics":
        {
          "disp":
          {
            "type":["min","max","mean"],
            "field":"displacement"
          },
          "von-mises": 
          {
            "type": ["min","max","mean"],
            "expr": "sqrt(0.5*((solid_stress_P_00-solid_stress_P_11)^2+(solid_stress_P_11-solid_stress_P_22)^2+(solid_stress_P_22-solid_stress_P_11)^2+6*(solid_stress_P_12^2+solid_stress_P_23^2+solid_stress_P_31^2))):solid_stress_P_00:solid_stress_P_11:solid_stress_P_22:solid_stress_P_12:solid_stress_P_23:solid_stress_P_31"
          }
        }         
      }
    }
  }
  // end::export[]
}
