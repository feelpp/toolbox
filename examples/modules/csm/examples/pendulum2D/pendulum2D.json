
// -*- mode: javascript -*-
{
  "Name": "Pendulum Solid Mechanics ",
  "ShortName":"Solid",
  "Models":
  {
      "equations":"Hyper-Elasticity",
      "material-model":"StVenantKirchhoff"
  },
  // tag::parameters[]
  "Parameters":
  {
    "gravity":"9.80665" // [m.s^{-2}]
  },
  //end::parameters[]
  // tag::materials[]
  "Materials":
  {
    "Solid":
    {
      "E":"128e6", // [kg.m^{-1}.s^{-2}]
      "nu":"0.33", // [dimensionless]
      "rho":"8920" // [kg/m^3]
    }
  },
  // end::materials[]
  // tag::boundaryconditions[]
  "BoundaryConditions":
  {
    "displacement":
    {
      "Dirichlet":
      {
        "AxeFixed": // vertex angle -> fixed (no displacement)
        {
          "expr":"{0,0}"
        }
      },

      "VolumicForces":
      {
        "Solid":
        {
          "expr":"{0,-gravity*8920}:gravity"
        }
      }
    }

  },
  // end::boundaryconditions[]
  // tag::exports[]
  "PostProcess":
  {
    "Exports":
    {
      "fields":["displacement","pid","von-mises-criterion"]
    },
    "Measures":
    {
      "VolumeVariation":""
    }
  }
  // end::exports[]
}
