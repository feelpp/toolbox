
// -*- mode: javascript -*-
{
  "Name": "Pendulum Solid Mechanics ",
  "ShortName":"Solid",
  "Metadata":
  {
      "version": "1",
      "author": ["vincentchabannes","prudhomm","lberti","jobinhio"],
      "date": "30/11/2022",
      "references":[""]
  },
  "Models":
  {
    "solid":
    {
      "setup":
      {
        "equations":"Hyper-Elasticity",
        "material-model":"StVenantKirchhoff",
        "body-forces":"{0,-gravity*8920}:gravity"
      }
    }
  },
  "Meshes":
  {
      "solid":
      {
        "Import":
        {
          "filename":"$cfgdir/pendulum2D.geo"
        }
      }
  },  
  // tag::parameters[]
  "Parameters":
  {
    "gravity":"9.80665" // [m.s^{-2}]
  },
  //end::parameters[]
  // tag::materials[]
  "Materials":
  {
    "Solid":
    {
      "E":"128e6", // [kg.m^{-1}.s^{-2}]
      "nu":"0.33", // [dimensionless]
      "rho":"8920" // [kg/m^3]
    }
  },
  // end::materials[]
  // tag::boundaryconditions[]
  "BoundaryConditions":
  {
    "solid":
    {
      "displacement":
      {
        "AxeFixed": // vertex angle -> fixed (no displacement)
        {
          "expr":"{0,0}"
        }
      }
    }

  },
  // end::boundaryconditions[]
  // tag::exports[]
  "PostProcess":
  {
    "solid":
    {
      "Exports":
      {
        "fields":["displacement","pid","von-mises-criterion"]
      },
      "Measures":
      {
          "VolumeVariation":"",
          "Statistics":
          {
              "disp":
              {
                  "type":["min","max","mean"],
                  "field":"displacement"
              },
              "von-mises": {
                  "type": ["min","max","mean"],
                  "expr": "sqrt(0.5*((solid_stress_P_00-solid_stress_P_11)^2+(solid_stress_P_11-solid_stress_P_22)^2+(solid_stress_P_22-solid_stress_P_11)^2+6*(solid_stress_P_12^2+solid_stress_P_23^2+solid_stress_P_31^2))):solid_stress_P_00:solid_stress_P_11:solid_stress_P_22:solid_stress_P_12:solid_stress_P_23:solid_stress_P_31"
              }
          }         
      }
    }
  } 
  // end::exports[]
}
