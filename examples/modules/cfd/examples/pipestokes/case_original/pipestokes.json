{
    "Name": "Stockes flow in a pipe",
    "ShortName":"pipestokes",
    "Models":
    {
        "equations":"Stokes"
    },
    "Parameters":
    {
        "ubar":"1.0",
        "height":"1.0",
        "vmax":"1.0"

    },
    "Materials":
    {
        "Fluid":{
            "rho":"1.0",
            "mu":"1.0"
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "inlet":
                {
                    "expr":"{(vmax/(height-(height/2.))*(height/2.))*y*(height-y),0}:y:height:vmax"
                },
                "wall1":
                {
                    "expr":"{0,0}"
                },
                "wall2":
                {
                    "expr":"{0,0}"
                }
            }
        },
        "fluid":
        {
            "outlet":
            {
                "outlet":
                {
                    "expr":"0" 
                }
            }
        }
    },
    "PostProcess":
    {
        "Exports":
        {
            "fields":["velocity","pressure","pid"]
        }
     
    }

}
