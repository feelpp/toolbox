// -*- mode: javascript -*-
{
    "Name":"HL-31_H1",
    "ShortName":"HL-31_H1",
    "Parameters":
    {
        "sigma0":53e3, //[ S/mm ]
        "T0":290, //[ K ]
        "alpha":3.6e-3, //[ 1/K ]
        "Lorentz":2.47e-8 //[ W*Omega/(K*K) ]
    },
    "Materials":
    {
        "Cu":
        {
            "k":"sigma0*Lorentz*heat_T/(1+alpha*(heat_T-T0)):sigma0:alpha:T0:Lorentz:heat_T", //[ W/(mm*K) ]
            "sigma":"sigma0/(1+alpha*(heat_T-T0))+0*heat_T:sigma0:alpha:T0:heat_T"// [S/mm ]
        }
    },
    "BoundaryConditions":
    {
        "temperature":
        {
            "Robin":
            {
                "Rint":
                {
                    "expr1":"0.085", // h [ K ]
                    "expr2":"290" // T_c [ W/(mm^2*K) ]
                },
                "Rext":
                {
                    "expr1":"0.085", // h [ K ]
                    "expr2":"290" // T_c [ W/(mm^2*K) ]
                }
            },
            "Neumann":
            {
                "Interface":
                {
                    "expr":"0"
                },
                "GR_1_Interface":
                {
                    "expr":"0"
                }
            }
        },
        "electric-potential":
        {
            "Dirichlet":
            {
                "V0":
                {
                    "expr":"0" // V_in [ V ]
                },
                "V1":
                {
                    "expr":"9" // V_out [ V ]
                }
            }
        }
    },
    "PostProcess":
    {
        "use-model-name":1,
        "thermo-electric":
        {
            "Exports":
            {
                "fields":["heat.temperature","electric.electric-potential","electric.electric-field","heat.pid"]
            }
        }
    }
}
