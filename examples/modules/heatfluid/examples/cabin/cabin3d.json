// -*- mode: javascript -*-
{
    "Name": "Fluid Mechanics",
    "ShortName":"Fluid",
    "Metadata":
    {
        "version": "1",
        "author": ["vincentchabannes","prudhomm","lberti","jobinhio"],
        "date": "28/11/2022",
        "references":[""]
    },
    "Models":
    {
        "fluid":
        {
            "equations":"Navier-Stokes"
        }
    },
    "Meshes":
    {
        "heatfluid":
        {
          "Import":
          {
            "filename":"$cfgdir/cabin3d.geo",
            "hsize":"0.2"  // 0.02 
          }
        }
    }, 
    "Parameters":
    {
    },
    "Materials":
    {
        "Fluid":{
            "rho":"1.0",
            "mu":"5.0e-4",
            "k":"1.45e-3",
            "Cp":"1",
            "beta":"0.003660" //0.00006900
        }
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "velocity":
            {
                "inlet" : {"expr":"{0,-0.75*(0.05-x)*(x+0.05)*(0.4-z)*(z+0.4)*2500*(1-exp(-5*t)),0}:x:y:z:t"},
                "passengers" : {"expr":"{0,0,0}"},
                "walls" : {"expr":"{0,0,0}"}
            },
            "outlet":
            {
                "outlet1":
                {
                    "expr":"0"
                },
                "outlet2":
                {
                    "expr":"0"
                }

            }
        },
        "heat":
        {
            "temperature":
            {
                "inlet" : {"expr":"20-5*(1-exp(-t)):t"}
                //"passengers" : {"expr":"20+15*(1-exp(-t)):t"}
            },
            "Neumann":
            {
                "passengers" : {"expr":"37.1e-3"}
            }
        }
    },
    "InitialConditions":
    {
        "temperature":
        {
            "":
            {
                "":
                {
                    "expr":"20"
                }
            }
        }
    },
    "PostProcess":
    {
        // "use-model-name":1,
        "heatfluid":
        {
            "Exports":
            {
                "fields":["fluid.velocity","fluid.pressure","heat.temperature","fluid.pid"]
            },
            "fluid":
            {
            },
            "heat":
            {
            }
        }
    }

}
